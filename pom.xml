    <properties>
        <ejiayou_common>1.0.0-SNAPSHOT</ejiayou_common>
        <platform-common.version>1.0.0-SNAPSHOT</platform-common.version>
        <platform-common-server.version>1.0.0-SNAPSHOT</platform-common-server.version>
